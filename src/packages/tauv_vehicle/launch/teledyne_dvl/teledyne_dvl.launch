<launch>
  <arg name="model_name" default="kingfisher"/>
  <arg name="config_pkg" default="$(eval find(arg('model_name') + '_description'))"/>

  <node pkg="tauv_vehicle" type="teledyne_dvl" name="teledyne_dvl" output="screen">
    <rosparam command="load" file="$(arg config_pkg)/dvl.yaml" />
    <remap from="dvl_data" to="/teledyne_dvl/data" />
    <remap from="imu_sync" to="/xsens_imu/sync" />
  </node>
</launch>
