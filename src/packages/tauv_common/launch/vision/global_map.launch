<launch>
    <arg name="config_pkg" default="$(eval find('kingfisher_description'))"/>
    <arg name="debug" default="none" />
    <rosparam command="load" file="$(arg config_pkg)/global_map.yaml" />

<!--     <group ns="global_map">-->
    <node
        pkg="tauv_common"
        type="global_map_cpp"
        name="global_map"
        output="screen"
        launch-prefix="$(eval 'gdb -ex \'break main\' -ex run --args' if arg('debug') == 'gdb' else 'valgrind --leak-check=full --track-origins=yes' if arg('debug') == 'valgrind' else 'valgrind --tool=callgrind --callgrind-out-file=/home/theo/catkin_ws/callgrind-out' if arg('debug') == 'callgrind' else '')"
    >
    </node>
    <node pkg="tauv_common" type="transform_detections" name="transform_detections" output ="screen" />
    <include file="$(find tauv_common)/launch/darknet_ros/darknet_ros.launch" />
<!--     </group>-->
</launch>