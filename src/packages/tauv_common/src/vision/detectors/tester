#!/usr/bin/env python3

import rospy
from tauv_msgs.msg import BucketList, BucketDetection
from visualization_msgs.msg import Marker, MarkerArray
import time

class Test:
    def __init__(self):
        rospy.init_node('tester', anonymous = True)
        self.bucket = rospy.Subscriber("bucket_list", BucketList, self.printer)
        #self.markers = rospy.Subscriber("camera_daemon/filtered_det_marker", MarkerArray, self.printer)

    def printer(self,data):
        for detection in data.bucket_list:
            rospy.loginfo(f"{detection}")

def listen():
    s = Test()

    rospy.spin()

if __name__=='__main__':
    listen()