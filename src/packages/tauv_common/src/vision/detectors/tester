#!/usr/bin/env python3

import rospy
from tauv_msgs.msg import BucketList, BucketDetection
import time

class Test:
    def __init__(self):
        rospy.init_node('tester', anonymous = True)
        self.bucket = rospy.Subscriber("bucket_list", BucketList, self.printer)

    def printer(self,data):
        rospy.loginfo(f"{data.bucket_list}")

def listen():
    s = Test()

    rospy.spin()

if __name__=='__main__':
    listen()