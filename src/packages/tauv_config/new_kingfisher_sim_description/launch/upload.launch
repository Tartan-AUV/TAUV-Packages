<launch>
    <arg name="x"     default="0"/>
    <arg name="y"     default="0"/>
    <arg name="z"     default="-2"/>
    <arg name="roll"  default="0"/>
    <arg name="pitch" default="0"/>
    <arg name="yaw"   default="1.57"/>

    <group ns="kingfisher">
        <param name="robot_description" command="$(find xacro)/xacro '$(find new_kingfisher_sim_description)/urdf/default.xacro'" />

       <node name="urdf_spawner" pkg="uuv_descriptions" type="spawn_model" respawn="false" output="screen"
             args="-urdf -x $(arg x) -y $(arg y) -z $(arg z) -R $(arg roll) -P $(arg pitch) -Y $(arg yaw) -model kingfisher -param /kingfisher/robot_description"/>
    </group>
</launch>