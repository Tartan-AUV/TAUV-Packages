<launch>
    <arg name="model_name" default="albatross"/>
    <arg name="config_pkg" default="$(eval find(arg('model_name') + '_description'))"/>

    <node name="keyboard_controller" pkg="tauv_common" type="keyboard_controller"
          output="screen" ns="$(arg model_name)/">
        <param name="output_frame" value="base_link"/>
    </node>

</launch>